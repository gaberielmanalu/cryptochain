/SearchResult.js","original":{"line":73,"column":17}},{"generated":{"line":132,"column":17},"source":"components/SearchResult.js","original":{"line":73,"column":19}},{"name":"clearList","generated":{"line":132,"column":22},"source":"components/SearchResult.js","original":{"line":73,"column":24}},{"generated":{"line":133,"column":0},"source":"components/SearchResult.js","original":{"line":71,"column":8}},{"generated":{"line":133,"column":16},"source":"components/SearchResult.js","original":{"line":52,"column":6}},{"generated":{"line":133,"column":17},"source":"components/SearchResult.js","original":{"line":51,"column":4}},{"generated":{"line":134,"column":0},"source":"components/SearchResult.js","original":{"line":79,"column":3}},{"name":"Component","generated":{"line":138,"column":2},"source":"components/SearchResult.js","original":{"line":7,"column":27}},{"name":"SearchResult","generated":{"line":140,"column":15},"source":"components/SearchResult.js","original":{"line":82,"column":15}}],"sources":{"components/SearchResult.js":"import React, { Component } from 'react';\r\nimport { Button } from 'react-bootstrap';\r\nimport Transaction from './Transaction';\r\nimport history from '../history';\r\n\r\n\r\nclass SearchResult extends Component {\r\n  state = { transactionPoolMap: {}, searchInfo: {} };\r\n\r\n  refreshToken =  () => {\r\n    fetch(`${document.location.origin}/api/refresh-token`)\r\n        .then(response => response.json())\r\n        .then(json => {\r\n        if (json.type === 'error'){\r\n          history.push('/');\r\n        }\r\n        });\r\n    }   \r\n\r\n\r\n  clearList = () => {\r\n    fetch(`${document.location.origin}/api/clear-list-search`)\r\n      .then(response => {\r\n        if (response.status === 200) {\r\n          alert('success');\r\n          history.push('/home');\r\n        } else {\r\n          alert('The clear transactions block request did not complete.');\r\n        }\r\n      });\r\n  }\r\n\r\n\r\n  componentDidMount() {\r\n    this.refreshToken();\r\n    fetch(`${document.location.origin}/api/get-searched-info`)\r\n      .then(response => response.json())\r\n      .then(json => this.setState({ searchInfo: json }));\r\n\r\n    fetch(`${document.location.origin}/api/get-result`)\r\n      .then(response => response.json())\r\n      .then(json => this.setState({ transactionPoolMap: json }));\r\n    \r\n    \r\n  }\r\n\r\n    \r\n  render() {\r\n    const  { searchedAddress , searchedBalance, searchedName} = this.state.searchInfo;\r\n\r\n    return (\r\n      <div className='SearchResult'>\r\n        <h3>Result</h3>\r\n          <div className='searchInfo'>\r\n          <div>Name: {searchedName}</div>\r\n          <div>Address: {searchedAddress}</div>\r\n          <div>Balance: {searchedBalance}</div>\r\n        </div>\r\n        <br/>\r\n        {\r\n          Object.values(this.state.transactionPoolMap).map(transaction => {\r\n            return (\r\n              <div key={transaction.id}>\r\n                <hr />\r\n                <Transaction transaction={transaction} />\r\n              </div>\r\n            )\r\n          })\r\n        }\r\n        <hr />\r\n        <Button\r\n          bsStyle=\"danger\"\r\n          onClick={this.clearList}\r\n        >\r\n          Home\r\n        </Button>\r\n      </div>\r\n    )\r\n  }\r\n}\r\n\r\nexport default SearchResult;"},"lineCount":null}},"error":null,"hash":"fa7552fa81faaa955eb0f69f0d0a8c22","cacheData":{"env":{}}}