his.setState({ username: event.target.value });\r\n  }\r\n\r\n  updatePassword = event => {\r\n    this.setState({ password: event.target.value });\r\n  }\r\n\r\n  updateRole = event => {\r\n    this.setState({ role: event.target.value });\r\n  }\r\n\r\n  checkData = () => {\r\n    const { username, password } = this.state;\r\n\r\n    fetch(`${document.location.origin}/api/login`, {\r\n      method: 'POST',\r\n      headers: { 'Content-Type': 'application/json' },\r\n      body: JSON.stringify({ username, password })\r\n    }).then(response => response.json())\r\n      .then(json => {\r\n        alert(json.message || json.type);\r\n        if (json.type === 'successAdmin'){\r\n          history.push('/admin');\r\n        } else if (json.type === 'successUser') {\r\n          history.push('/home');\r\n        } else {\r\n          history.push('/login');\r\n        }\r\n        \r\n      });\r\n  }\r\n  \r\n  render() {\r\n    const {role}= this.state;\r\n    return (\r\n      <div className='Login'>\r\n        <label>Username: </label>\r\n        <FormGroup>\r\n          <FormControl\r\n            input='text'\r\n            placeholder='username'\r\n            value={this.state.username}\r\n            onChange={this.updateUsername}\r\n          />\r\n        </FormGroup>\r\n\r\n        <label>Password: </label>\r\n        <FormGroup>\r\n          <FormControl\r\n            input='password'\r\n            type='password'\r\n            placeholder='*******'\r\n            value={this.state.password}\r\n            onChange={this.updatePassword}\r\n          />\r\n        </FormGroup>\r\n        <div>\r\n          <Button\r\n            bsStyle=\"danger\"\r\n            onClick={this.checkData}\r\n          >\r\n            Login\r\n          </Button>\r\n          <br/>\r\n          <Link to='/signup'>Signup</Link>\r\n        </div>\r\n      </div>\r\n    )\r\n  }\r\n};\r\n\r\nexport default Login;"},"lineCount":null}},"error":null,"hash":"6916821b1e6dcf6cb6c23d